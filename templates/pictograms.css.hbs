[class^="{{prefix}}-"],
[class*=" {{prefix}}-"] {
display: inline-block;
width: 1em;
height: 1em;
background-repeat: no-repeat;
background-position: center;
background-size: contain;
vertical-align: middle;
}


{{# each pictograms }}
.{{ ../prefix }}-{{ name }} {
  background-image: url("{{{ cssUrl }}}");
}
{{/ each }}
