{
  "name": "iconomatic",
  "version": "0.0.0-dev",
  "description": "Automation tool to create icon library",
  "license": "MIT",
  "author": "Abydos12 <38505162+Abydos12@users.noreply.github.com>",
  "homepage": "https://github.com/Abydos12/iconomatic#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abydos12/iconomatic.git"
  },
  "bugs": {
    "url": "https://github.com/Abydos12/iconomatic/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "iconomatic": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup src",
    "clean": "rimraf dist",
    "dev": "tsx src/index.ts",
    "prepack": "npm run build",
    "test:many-icons": "npm pack && cd tests/many-icons && npm install ../../iconomatic-0.0.0-dev.tgz && npx iconomatic",
    "test:custom-config": "npm pack && cd tests/custom-config && npm install ../../iconomatic-0.0.0-dev.tgz && npx iconomatic"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.412",
    "@iconify/tools": "^5.0.0",
    "@iconify/types": "^2.0.0",
    "@tsconfig/node-lts": "^24.0.0",
    "@tsconfig/node-ts": "^23.6.2",
    "@tsconfig/strictest": "^2.0.8",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash.defaultsdeep": "^4.6.9",
    "@types/node": "^24.10.1",
    "@types/svg2ttf": "^5.0.3",
    "prettier": "^3.7.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "icons"
  ],
  "peerDependencies": {
    "cosmiconfig": "^9.0.0",
    "fs-extra": "^11.3.1",
    "handlebars": "^4.7.8",
    "lodash.defaultsdeep": "^4.6.1",
    "svg2ttf": "^6.0.3",
    "svgicons2svgfont": "^15.0.1",
    "ttf2woff2": "^8.0.0",
    "valibot": "^1.1.0"
  }
}
